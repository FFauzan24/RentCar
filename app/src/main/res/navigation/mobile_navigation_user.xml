<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_grid_mobil">

    <fragment
        android:id="@+id/navigation_grid_mobil"
        android:name="com.acenkzproject.rentcar.Fragment.user.FragmentGridMobil">
        <argument
            android:name="username"
            app:argType="string"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_transaksi"
        android:name="com.acenkzproject.rentcar.Fragment.user.FragmentListTransaksi">
        <argument
            android:name="username"
            app:argType="string"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.acenkzproject.rentcar.Fragment.user.FragmentProfile">
        <argument
            android:name="username"
            app:argType="string"/>
    </fragment>

</navigation>